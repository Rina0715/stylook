<div class="container">
  <div class="row">
    <div class="col-md-4 offset-md-1">
      <h6>シーズン編集</h6>
    </div>
  </div>
  <%= form_with model:@season, url: season_path(@season.id), local:true do |f| %>
  <div class="row">
    <div class="col-md-2">シーズン名</div>
    <div class="col-md-3"><%= f.text_field :name %></div>
    <div class="col-md-2"><%= f.submit '変更を保存', class: "btn btn-outline-secondary" %></div>
  </div>
  <% end %>
</div>